    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/md5": "^2.3.2",
    "@types/qs": "^6.9.14",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "jiti": "^2.6.1",
    "sass": "^1.93.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
