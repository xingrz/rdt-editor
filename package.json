    "build": "vite build",
    "lint": "eslint --ext .ts,.vue src"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "ace-code": "^1.32.8",
    "md5": "^2.3.0",
    "pinia": "^3.0.3",
    "qs": "^6.12.0",
